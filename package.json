    "buildProdLinux": "rm -rf dist && webpack --mode production --config webpack.server.config.js && webpack --mode production --config webpack.prod.config.js",
    "start": "node ./dist/server.js",
    "install-fontello": "fontello-cli install --css ./src/fontello/css --font ./src/fontello/font"
